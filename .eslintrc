{
    "plugins": [
        "react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "linebreak-style": [
            "off",
            "unix"
        ],
        "quotes": [
            "off",
            "single"
        ],
        "semi": [
            "warn",
            "never"
        ],
        "no-undef": [
            "off"
        ],
        "no-unreachable": [
            "warn"
        ],
        "no-unused-vars": 0,
        "no-console": 0
    }
}
